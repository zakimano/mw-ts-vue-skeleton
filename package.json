{
  "name": "ts-vue-skeleton",
  "version": "2.0.0",
  "description": "ts-vue-skeleton",
  "main": "./mwdist/index.js",
  "scripts": {
    "clean": "node util/clean_mwdist.js",
    "build:ts": "tsc",
    "build:ui": "cd ui && npm i && npm run build",
    "watch:ts": "tsc -w",
    "watch:ui": "cd ui && npm i && npm run watch",
    "webpack:build": "webpack",
    "webpack:watch": "webpack --watch",
    "watch-ts": "npm run clean && concurrently npm:watch:*",
    "watch-pack": "npm run clean && concurrently npm:watch:ui npm:webpack:watch",
    "build": "npm run clean && npm run webpack:build && npm run build:ui"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@mxw/next": "^1.6.2",
    "@mxw/three": "^1.0.4",
    "@mxw/three-utils": "^1.0.2",
    "@mxw/types": "^1.0.17",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.192",
    "@types/node": "^16.18.23",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "concurrently": "^6.5.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.7",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  }
}
